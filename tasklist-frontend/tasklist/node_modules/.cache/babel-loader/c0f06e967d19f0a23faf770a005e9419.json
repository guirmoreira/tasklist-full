cAAA,EAAA,WAAA,CARA;AASA,MAAA,MAAA,EAAA,IAAA,IAAA,CAAA,IAAA,CAAA,GAAA,KAAA,IAAA,IAAA,GAAA,iBAAA,KAAA,KAAA,EACA,WADA,GAEA,MAFA,CAEA,CAFA,EAEA,EAFA,CATA;AAYA,MAAA,IAAA,EAAA,EAZA;AAaA,MAAA,IAAA,EAAA,KAbA;AAcA,MAAA,KAAA,EAAA,KAdA;AAeA,MAAA,KAAA,EAAA;AAfA,KAAA;AAAA,GAHA;AAqBA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,WAAA,IAAA,CAAA,cAAA,GAAA,KAAA,MAAA;AACA,WAAA,IAAA,GAAA,KAAA,UAAA,CAAA,KAAA,MAAA,CAAA;AACA;AAJA,GArBA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AAAA;;AACA,MAAA,KAAA,CAAA,UAAA,CAAA,KAAA,IAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,YAAA;AACA,OAHA;AAIA,WAAA,MAAA,GAAA,KAAA;AACA,KAPA;AAQA,IAAA,UARA,sBAQA,MARA,EAQA;AACA,0BAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AAAA;AAAA,UAAA,IAAA;AAAA,UAAA,KAAA;AAAA,UAAA,GAAA;;AACA,uBAAA,GAAA,cAAA,KAAA,cAAA,IAAA;AACA;AAXA;AA5BA,CAAA","sourcesContent":["<template>\r\n  <v-dialog max-width=\"900px\" v-model=\"dialog\">\r\n    <template v-slot:activator=\"{ on, attrs }\">\r\n      <v-btn\r\n        icon\r\n        color=\"grey darken-1\"\r\n        slot=\"activation\"\r\n        v-bind=\"attrs\"\r\n        v-on=\"on\"\r\n      >\r\n        <v-icon> mdi-pencil </v-icon>\r\n      </v-btn>\r\n    </template>\r\n\r\n    <v-card>\r\n      <v-card-title class=\"pb-8\">\r\n        <h5 class=\"text--subtitle\">EDITAR TAREFA</h5>\r\n      </v-card-title>\r\n\r\n      <v-card-text>\r\n        <v-text-field\r\n          label=\"Título\"\r\n          v-model=\"task.title\"\r\n          filled\r\n          dense\r\n          clearable\r\n          required\r\n          maxlength=\"64\"\r\n        ></v-text-field>\r\n        <v-text-field\r\n          label=\"Descrição\"\r\n          v-model=\"task.description\"\r\n          filled\r\n          dense\r\n          clearable\r\n          required\r\n          maxlength=\"256\"\r\n        >\r\n          <template v-slot:label>\r\n            <div>Descrição <small>(opcional)</small></div>\r\n          </template>\r\n        </v-text-field>\r\n        <v-select\r\n          label=\"Status\"\r\n          v-model=\"task.status\"\r\n          :items=\"items\"\r\n          filled\r\n        ></v-select>\r\n        <v-menu\r\n          ref=\"menu\"\r\n          v-model=\"menu\"\r\n          :close-on-content-click=\"false\"\r\n          :return-value.sync=\"date\"\r\n          transition=\"scale-transition\"\r\n          offset-y\r\n          min-width=\"auto\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-text-field\r\n              v-model=\"date\"\r\n              label=\"Data de Entrega\"\r\n              prepend-icon=\"mdi-calendar\"\r\n              readonly\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            ></v-text-field>\r\n          </template>\r\n          <v-date-picker v-model=\"picker\" no-title scrollable>\r\n            <v-spacer></v-spacer>\r\n            <v-btn text color=\"primary\" @click=\"menu = false\"> Cancelar </v-btn>\r\n            <v-btn text color=\"primary\" @click=\"$refs.menu.save(date)\">\r\n              OK\r\n            </v-btn>\r\n          </v-date-picker>\r\n        </v-menu>\r\n      </v-card-text>\r\n\r\n      <v-card-actions>\r\n        <v-btn color=\"red darken-1\" text @click=\"dialog = false\">\r\n          Cancelar\r\n        </v-btn>\r\n\r\n        <v-spacer></v-spacer>\r\n        <v-btn color=\"green darken-1\" text @click=\"saveTask\"> Salvar </v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport tasks from \"../../services/tasks\";\r\nexport default {\r\n  props: [\"parentTask\"],\r\n\r\n  data: () => ({\r\n    dialog: false,\r\n    task: {\r\n      title: this.$props.parentTask.title,\r\n      description: this.$props.parentTask.description,\r\n      status: this.$props.parentTask.status,\r\n      dateConclusion: this.$props.parentTask.dateConclusion,\r\n    },\r\n    items: [\"ABERTA\", \"EM_ANDAMENTO\", \"CONCLUÍDA\"],\r\n    picker: new Date(Date.now() - new Date().getTimezoneOffset() * 60000)\r\n      .toISOString()\r\n      .substr(0, 10),\r\n    date: \"\",\r\n    menu: false,\r\n    modal: false,\r\n    menu2: false,\r\n  }),\r\n\r\n  watch: {\r\n    picker() {\r\n      this.task.dateConclusion = this.picker;\r\n      this.date = this.formatDate(this.picker);\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    saveTask() {\r\n      tasks.createTask(this.task).then(() => {\r\n        this.$parent.list();\r\n        this.$parent.$forceUpdate;\r\n      });\r\n      this.dialog = false;\r\n    },\r\n    formatDate(picker) {\r\n      const [year, month, day] = picker.split(\"-\");\r\n      return `${day}/${month}/${year}`;\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/popups"}]}