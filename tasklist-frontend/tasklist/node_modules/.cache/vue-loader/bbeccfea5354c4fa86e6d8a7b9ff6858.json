rable\r\n              required\r\n              maxlength=\"64\"\r\n            ></v-text-field>\r\n            <v-text-field\r\n              label=\"Descrição\"\r\n              v-model=\"task.description\"\r\n              filled\r\n              dense\r\n              clearable\r\n              required\r\n              maxlength=\"256\"\r\n            >\r\n              <template v-slot:label>\r\n                <div>Descrição <small>(opcional)</small></div>\r\n              </template>\r\n            </v-text-field>\r\n            <v-select\r\n              label=\"Status\"\r\n              v-model=\"task.status\"\r\n              :items=\"items\"\r\n              filled\r\n            ></v-select>\r\n          </v-col>\r\n          <v-col cols=\"4\">\r\n            <v-date-picker v-model=\"picker\"></v-date-picker>\r\n          </v-col>\r\n        </v-row>\r\n      </v-card-text>\r\n\r\n      <v-card-actions>\r\n        <v-btn color=\"red darken-1\" text @click=\"dialog = false\">\r\n          Cancelar\r\n        </v-btn>\r\n\r\n        <v-spacer></v-spacer>\r\n        <v-btn color=\"green darken-1\" text @click=\"saveTask\"> Salvar </v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport tasks from \"../../services/tasks\";\r\nexport default {\r\n  data: () => ({\r\n    dialog: false,\r\n    task: {\r\n      title: \"\",\r\n      description: \"\",\r\n      status: \"\",\r\n      dateConclusion: \"\",\r\n    },\r\n    items: [\"ABERTA\", \"EM_ANDAMENTO\", \"CONCLUIDA\"],\r\n    picker: new Date(Date.now() - new Date().getTimezoneOffset() * 60000)\r\n      .toISOString()\r\n      .substr(0, 10),\r\n  }),\r\n\r\n  watch: {\r\n    picker() {\r\n      this.task.dateConclusion = this.picker;\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    saveTask() {\r\n      tasks.createTask(this.task).then((response) => {\r\n        alert(response);\r\n      }),\r\n        (this.dialog = false);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}